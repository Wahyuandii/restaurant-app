{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jasmine": true,
        "codeceptjs/codeceptjs": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:codeceptjs/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "func-names": "off",
        "consistent-return": 0,
        "import/prefer-default-export": 0,
        "no-prototype-builtins": 0,
        "no-return-assign": 0
    },
    "plugins": ["jasmine", "codeceptjs"]
}
